<%= link_to project_path(@project), class: 'btn btn-secondary mb-3' do %>
  <i class="fas fa-arrow-left"></i> Back to Project
<% end %>

<h1>Tasks for <%= @project.name %></h1>
<p>Find me in app/views/tasks/index.html.erb</p>


<% @tasks.each do |task| %>

<ul>
  <li><%= task.description %></li>
</ul>

<% end %>
